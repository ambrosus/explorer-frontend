{
  "name": "airdao-explorer-frontend",
  "version": "1.0.1",
  "private": true,
  "homepage": "/explorer",
  "scripts": {
    "start": "yarn format && react-scripts start",
    "build": "yarn configure && react-scripts build",
    "build:dev": "react-scripts build",
    "configure": "./preinstall.sh",
    "test": "react-scripts test",
    "serve": "react-scripts build && serve -s build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "format": "prettier --write \"{,!(node_modules)/**/}*.(ts|tsx)\""
  },
  "eslintConfig": {
    "extends": "@inc4/react"
  },
  "prettier": "@inc4/prettier-config",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@airdao/airdao-node-contracts": "^1.0.9",
    "@babel/plugin-transform-react-jsx": "^7.17.12",
    "@babel/plugin-transform-react-jsx-self": "^7.17.12",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/react-helmet": "^6.1.6",
    "@web3-react/core": "^8.2.0",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "^6.2.13",
    "airdao-components-and-tools": "https://github.com/ambrosus/airdao-components-and-tools#web3-react-migration",
    "autoprefixer": "^10.4.5",
    "axios": "^0.26.1",
    "eslint": "^8.14.0",
    "ethers": "^5.0.0",
    "jest": "^26.0.1",
    "moment": "^2.29.1",
    "node-sass": "^7.0.1",
    "react-date-range": "^1.4.0",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "^9.1.0",
    "react-query": "^3.39.1",
    "react-redux": "^7.2.8",
    "react-scripts": "5.0.0",
    "react-tooltip": "^4.2.21",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "sass": "^1.49.9",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@inc4/eslint-config-react": "1.0.1",
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.22",
    "@types/react": "^18.0.9",
    "@types/react-date-range": "^1.4.3",
    "@types/react-dom": "^18.0.3",
    "@types/react-query": "^1.2.9",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "5.3.3",
    "@types/redux-promise": "^0.5.29",
    "@types/testing-library__jest-dom": "^5.14.3",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "prettier": "2.6.0",
    "pretty-quick": "3.1.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-error-overlay": "^6.0.9",
    "react-router-dom": "6.2.2",
    "react-testing-library": "^8.0.1"
  }
}
